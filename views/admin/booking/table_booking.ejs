<!-- DataTables -->
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>No</th>
            <th>Invoice</th>
            <th>Date</th>
            <th>Title Item</th>
            <th>Member Name</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% booking.forEach((data, index) => { %>
            <tr>
              <td><%= ++index %></td>
              <td>#<%= data.invoice %></td>
              <td><%= data.bookingStartDate.toLocaleString().split(',')[0] %> s/d <%= data.bookingEndDate.toLocaleString().split(',')[0] %></td>
              <td><%= data.itemId.title %></td>
              <td><%= data.memberId.firstName %> <%= data.memberId.lastName %></td>
              <td><%= data.payments.status %></td>
              <td class="text-center">
                <a class="btn btn-success" href="/admin/booking/<%= data._id %>"><i class="fas fa-eye"></i></a>
              </td>
            </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<!-- End DataTables -->
