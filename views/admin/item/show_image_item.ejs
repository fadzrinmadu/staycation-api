<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="show-image-tab">
  <div class="row">
    <% item.imageId.forEach((image) => { %>
      <div class="col-md-4">
        <div class="card shadow mb-4 mt-2">
          <div class="card-body">
            <img src="/<%= image.imageUrl %>" alt="image item" style="width: 100%; display: block">
          </div>
        </div>
      </div>
    <% }) %>
  </div>
  <a href="/admin/item" class="btn btn-warning">Back</a>
</div>

